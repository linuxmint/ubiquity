#. Description
#: ../ubiquity.templates:30001
msgid "Username"
msgstr "Korisničko ime"

#. Type: text
#. Description
#: ../ubiquity.templates:31001
msgid "Choose a password:"
msgstr "Pdaberite lozinku:"

#. Type: text
#. Description
#: ../ubiquity.templates:32001
msgid "Password"
msgstr "Šifra"

